{
  "name": "react-template",
  "version": "1.0.0",
  "description": "",
  "main": "server/index.js",
  "license": "Apache-2.0",
  "scripts": {
    "start": "node server/index.js",
    "build-dev": "webpack --progress --config ./client/config/webpack.dev.js --mode development",
    "build-prod": "webpack -p --progress --config ./client/config/webpack.prod.js --mode production",
    "dev": "nodemon --watch server server/index.js & yarn build-dev --watch",
    "prod": "nodemon --watch server/index.js & yarn build-prod --watch",
    "test": "jest --watch --noStackTrace",
    "postinstall": "webpack -p --progress --config ./client/config/webpack.prod.js --mode production"
  },
  "engines": {
    "node": "^10.0.0",
    "yarn": ">= 0.18.0"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-3"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^3.4.0",
    "@material-ui/icons": "^3.0.1",
    "@types/googlemaps": "^3.30.16",
    "@types/markerclustererplus": "^2.1.33",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "body-parser": "^1.18.3",
    "css-loader": "^1.0.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "knex": "^0.15.2",
    "node": "^10.7.0",
    "node-sass": "^4.9.2",
    "nodemon": "^1.18.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-google-maps": "^9.4.5",
    "react-router-dom": "^4.3.1",
    "sass-loader": "^7.0.3",
    "sqlite3": "^4.0.4",
    "style-loader": "^0.21.0",
    "superagent": "^4.0.0",
    "webpack": "^4.16.3",
    "webpack-merge": "^4.1.4"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "nodemon": "^1.18.3",
    "webpack-cli": "^3.1.0"
  }
}
